{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "seed-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/seed-app",
      "sourceRoot": "projects/seed-app/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/seed-app/**/*.ts",
              "projects/seed-app/**/*.html"
            ]
          }
        },
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist",
              "browser": ""
            },
            "index": "projects/seed-app/src/index.html",
            "browser": "projects/seed-app/src/main.ts",
            "polyfills": [],
            "tsConfig": "projects/seed-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/seed-app/public"
              },
              {
                "glob": "README.md",
                "input": ".",
                "output": "/"
              }
            ],
            "styles": [
              "projects/seed-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "seed-app:build:production"
            },
            "development": {
              "buildTarget": "seed-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        }
      }
    },
    "seed-common-lib": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
            "style": "scss"
        }
      },
      "root": "projects/seed-common-lib",
      "sourceRoot": "projects/seed-common-lib/src",
      "prefix": "lib",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/seed-common-lib/**/*.ts",
              "projects/seed-common-lib/**/*.html"
            ]
          }
        },
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/seed-common-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/seed-common-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/seed-common-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "global-error-handler-lib": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/global-error-handler-lib",
      "sourceRoot": "projects/global-error-handler-lib/src",
      "prefix": "lib",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/global-error-handler-lib/**/*.ts",
              "projects/global-error-handler-lib/**/*.html"
            ]
          }
        },
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/global-error-handler-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/global-error-handler-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
